config {
  type: "operations",
  schema: "holidays_in_japan__us",
  tags: ["US"],
  hasOutput: true,
}

CREATE OR REPLACE FUNCTION ${self()}(d DATE)
  AS (
    "2007-01-01" <= d AND EXTRACT(MONTH FROM d) = 4 AND EXTRACT(DAY FROM d) = 29
  )
  OPTIONS (
    description = "昭和の日 4月29日 激動の日々を経て、復興を遂げた昭和の時代を顧み、国の将来に思いをいたす。"
  )
